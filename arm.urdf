<?xml version="1.0"?>
<robot xmlns:drake="http://drake.mit.edu" name = "arm">


	<link name ="base_z">
		<visual>
			<geometry>
				<sphere radius="0.025"/>
			</geometry>
			<origin rpy="0 0 0"/>
			<material>
				<color rgba="0.5 0.5 0.5 0.5"/>
			</material>
		</visual>
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="1"/>
			<inertia ixx="0.0001"  ixy="0"  ixz="0" iyy="0.0001" iyz="0" izz="0.0001" />
		</inertial>
	</link>


	<link name ="base_x">
		<visual>
			<geometry>
				<sphere radius="0.025"/>
			</geometry>
			<origin rpy="0 0 0"/>
			<material>
				<color rgba="0.5 0.5 0.5 0.5"/>
			</material>
		</visual>
		<inertial>
			<origin xyz="0 0 0"/>
			<mass value="1"/>
			<inertia ixx="0.0001"  ixy="0"  ixz="0" iyy="0.0001" iyz="0" izz="0.0001" />
		</inertial>
	</link>

		<joint name = "base" type = "continuous">
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<parent link="base_z"/>
			<child link="base_x"/>
			<axis xyz="0 0 1"/>
		</joint>

	<link name ="upper">
		<visual>
			<geometry>
				<box size=".5715 .0381 0.0762"/>
			</geometry>
			<origin xyz="0.2667 0 0"/>
			<material name="Green2">
				<color rgba="0.1 0.8 0 1.0"/>
			</material>
		</visual>

		<inertial>
			<origin xyz="0.18923 0 0" rpy="0 0 0"/>
			<mass value="0.840"/>
			<inertia ixx=".000527"  ixy="0"  ixz="0" iyy="0.02581" iyz="0" izz="0.02549" />
		</inertial>

		<collision>
			<origin xyz="0.2667 0 0" rpy="0 0 0"/>
			<geometry>
				<box size=".5715 .0381 0.0762"/>
			</geometry>
		</collision>

	</link>

	<joint name = "shoulder" type = "continuous">
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<parent link="base_x"/>
		<child link="upper"/>
		<axis xyz="0 1 0"/>
	</joint>


	<link name ="lower">
		<visual>
			<geometry>
				<box size=".381 .0254 0.0508"/>
			</geometry>
			<origin xyz = "0.1905 0 0"/>
			<material>
				<color rgba="0 0.9 0.9 1.0"/>
			</material>
		</visual>

		<inertial>
			<origin xyz="0.14732 0 0"/>
			<mass value="0.3195"/>
			<inertia ixx="0.00009434"  ixy="0"  ixz="0" iyy="0.00449921" iyz="0" izz="0.00443922" />
		</inertial>

		<collision>
			<origin xyz="0.1905 0 0"/>
			<geometry>
				<box size=".381 .0254 0.0508"/>
			</geometry>
		</collision>

	</link>


	<joint name = "elbow" type = "continuous">
		<origin xyz="0.508 0 0" rpy="0 0 0"/>
		<parent link="upper"/>
		<child link="lower"/>
		<axis xyz="0 1 0"/>
	</joint>
</robot>
