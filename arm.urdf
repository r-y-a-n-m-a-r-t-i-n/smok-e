<? xml version="1.0"?>
<robot name = "arm">
    <link name ="base">
        <visual>
            <geometry>
                <sphere radius="0.25"/>
            <origin>
            <material>
        </visual>

        <collision>
            <geometry>
            <origin>
        </collision>

        <inertial>
            <mass>
            <origin>
            <inertia>
        </inertial>
    </link>

    <link name ="upper">
        <visual>
            <geometry>
                <box size="1 1 3"/>
            <origin>
            <material>
        </visual>

        <collision>
            <geometry>
            <origin>
        </collision>

        <inertial>
            <mass>
            <origin>
            <inertia>
        </inertial>
    </link>

    <joint name = "base" type = "revolute">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="base"/>
        <child link="upper"/>
        <axis xyz="0 0 1"/>
    </joint>

    <joint name = "shoulder" type = "revolute">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="base"/>
        <child link="upper"/>
        <axis xyz="0 1 0"/>
    </joint>

    <link name = "lower">
        <visual>
            <geometry>
                <box size="1 1 3"/>
            <origin>
            <material>
        </visual>

        <collision>
            <geometry>
            <origin>
        </collision>

        <inertial>
            <mass>
            <origin>
            <inertia>
        </inertial>
    </link>

    <joint name = "elbow" type = "revolute">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="upper"/>
        <child link="lower"/>
        <axis xyz="0 1 0"/>
    </joint>

</robot>